{
  "private": true,
  "name": "@dcgw/you-are-now-in-space",
  "version": "0.0.0-development",
  "description": "A game for Mini Ludum Dare 25",
  "keywords": [
    "game",
    "typescript",
    "ludum-dare",
    "mini-ludum-dare-25",
    "excalibur"
  ],
  "contributors": [
    {
      "name": "Daniel Cassidy",
      "email": "mail@danielcassidy.me.uk",
      "url": "https://www.danielcassidy.me.uk/"
    },
    {
      "name": "George Weller",
      "email": "george.weller@gmail.com"
    },
    {
      "name": "Ian Johnson",
      "email": "git@simplyian.net"
    }
  ],
  "license": "UNLICENSED",
  "bugs": "https://github.com/dcgw/you-are-now-in-space-ts/issues",
  "homepage": "https://github.com/dcgw/you-are-now-in-space-ts",
  "repository": "github:dcgw/you-are-now-in-space-ts",
  "scripts": {
    "test": "tsc && tslint --project .",
    "build": "webpack --env.production",
    "semantic-release": "semantic-release",
    "start": "webpack-dev-server --open"
  },
  "devDependencies": {
    "@semantic-release/exec": "5.0.0",
    "@softwareventures/tslint-rules": "0.9.2",
    "@softwareventures/webpack-config": "3.0.3",
    "cz-conventional-changelog": "3.3.0",
    "semantic-release": "17.4.2",
    "semantic-release-s3-upload": "0.2.80",
    "ts-loader": "8.1.0",
    "tslint": "6.1.3",
    "typescript": "4.2.3",
    "webpack": "5.30.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@types/domready": "1.0.0",
    "@types/webpack-env": "1.16.0",
    "dictionary-types": "1.2.0",
    "domready": "1.0.8",
    "excalibur": "0.24.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "yarn build"
        }
      ],
      [
        "semantic-release-s3-upload",
        {
          "uploads": [
            {
              "src": {
                "dir": "./dist"
              },
              "dest": {
                "bucket": "you-are-now-in-space"
              }
            }
          ]
        }
      ]
    ]
  }
}
